{
  "name": "@emmetio/snippets-registry",
  "version": "0.1.1",
  "description": "JavaScript module for storing and retrieving Emmet snippets",
  "main": "dist/snippets-registry.cjs.js",
  "jsnext:main": "dist/snippets-registry.es.js",
  "scripts": {
    "test": "mocha",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "rollup -c -o dist/snippets-registry.cjs.js -f cjs ./index.js",
    "build:es": "rollup -c -o dist/snippets-registry.es.js -f es ./index.js",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmetio/snippets-registry.git"
  },
  "keywords": [],
  "author": "Sergey Chikuyonok <serge.che@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emmetio/snippets-registry/issues"
  },
  "homepage": "https://github.com/emmetio/snippets-registry#readme",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-register": "^6.18.0",
    "mocha": "^3.1.2",
    "rollup": "^0.41.1"
  }
}
